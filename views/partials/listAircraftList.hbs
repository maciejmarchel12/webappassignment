<div class="ui centered cards">
  {{#each aircraftList}}
    <div class="ui card">
      <div class="image">
        <img src="{{Image}}">
      </div>
      <div class="content">
        <a class="header">{{uppercase title}}</a>
      </div>
      <div class="metadata">
        <span class="date">Created: {{formatDate date}}</span>
      </div>
      <div class="extra content">
        <a href="/aircraft/{{id}}" class="ui icon button">
          <i class="icon folder open"></i>
        </a>
        <a href="/dashboard/deleteAircraftList/{{id}}" class="ui icon button">
          <i class="icon trash"></i>
        </a>
      </div>
    </div>
  {{/each}}
</div>